{
    "appenders": [
        {
            "type": "console",
            "layout": {
                "type": "pattern",
                "pattern": "%d{ISO8601} - %-5p - %c - %m"
            }
        },
        {
            "type": "dateFile",
            "filename": "log/nemfx.log",
            "pattern": ".yyyy-MM-dd",
            "alwaysIncludePattern": false,
            "layout": {
                "type": "pattern",
                "pattern": "%d{ISO8601} - %-5p - %-20c - %m"
            }
        }
    ],
    "levels": {
        "[all]": "TRACE"
    },
    "replaceConsole": true
}